<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <title>Student Grade Evaluator</title>
</head>
<body>

    <h1>Student Grade Evaluator</h1>

    <script>

        let marks = 72;
        let grade;

        if (marks >= 75) {
            grade = "Grade A";
        }
        else if (marks >= 60) {
            grade = "Grade B";
        }
        else if (marks >= 40) {
            grade = "Grade C";
        }
        else {
            grade = "Fail";
        }

        console.log("Marks:", marks);
        console.log("Grade:", grade);

    </script>

</body>
</html>
